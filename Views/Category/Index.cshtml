@using BulkWeb.Controllers;
@model List<Category>
<h1>Category List</h1>
<table class="table table-bordered table-string table-striped" >
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Display Order</th>
        </tr>

    </thead>

    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.Id</td>
            </tr>
        }
    </tbody>

</table>